<?xml version="1.0" encoding="utf-8"?>
<particles>


    <texture>art/textures/particles/cloud_1_512.png</texture>

    <blend mode="mix"/>


    <start_full/>


    <constant name="emissionrate" value="5.0"/>

    <uniform name="lifetime" min="5.0" max="5.5"/>


    <uniform name="angle" min="-0.5" max="0.5"/>


    <uniform name="velocity.x" min="-0.3" max="0.3"/>

    <uniform name="velocity.y" min="0.3" max="0.4"/>

    <uniform name="velocity.z" min="1.3" max="2.3"/>

    <uniform name="velocity.angle" min="-0.5" max="0.5"/>


    <uniform name="size" min="3.5" max="5.0"/>


    <uniform name="color.r" min="0.8" max="1.0"/>

    <copy name="color.g" from="color.r"/>

    <copy name="color.b" from="color.r"/>


</particles>
